{
  "name": "private-traffic-analytics",
  "version": "1.0.0",
  "description": "Private Traffic Data Aggregation using Zama FHE encryption",
  "main": "public/index.html",
  "scripts": {
    "start": "npx http-server public -p 3000 -c-1 --cors",
    "build": "echo 'Static site - no build needed'",
    "deploy": "vercel --prod",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:watch": "hardhat test --watch",
    "test:sepolia": "hardhat test --network sepolia",
    "test:coverage": "hardhat coverage",
    "test:gas": "REPORT_GAS=true hardhat test",
    "test:all": "npm run test && npm run test:coverage && npm run test:gas",
    "lint": "npm run lint:sol && npm run lint:ts",
    "lint:sol": "solhint 'contracts/**/*.sol'",
    "lint:ts": "eslint . --ext .ts,.tsx",
    "lint:fix": "npm run lint:sol -- --fix && npm run lint:ts -- --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,sol}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,sol}\"",
    "clean": "hardhat clean",
    "node": "hardhat node",
    "deploy:local": "hardhat run scripts/deploy.js --network localhost",
    "deploy:sepolia": "hardhat run scripts/deploy.js --network sepolia",
    "deploy:v2": "hardhat run scripts/deploy-v2.js --network sepolia",
    "verify:sepolia": "hardhat run scripts/verify.js --network sepolia",
    "interact": "hardhat run scripts/interact.js --network sepolia",
    "simulate": "hardhat run scripts/simulate.js --network sepolia",
    "size": "hardhat size-contracts",
    "typechain": "hardhat typechain",
    "security": "npm audit && slither . --config-file slither.config.json",
    "security:check": "npm audit --audit-level=moderate",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "ci": "npm run lint && npm run compile && npm test && npm run test:coverage"
  },
  "keywords": [
    "blockchain",
    "fhe",
    "privacy",
    "traffic",
    "zama",
    "hardhat"
  ],
  "author": "Private Traffic Analytics",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@fhevm/hardhat-plugin": "^0.1.0",
    "@fhevm/mock-utils": "^0.1.0",
    "@fhevm/solidity": "^0.8.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-ignition": "^0.15.13",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.14",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@nomicfoundation/ignition-core": "^0.15.13",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@zama-fhe/oracle-solidity": "^0.1.0",
    "@zama-fhe/relayer-sdk": "^0.2.0",
    "audit-ci": "^6.6.1",
    "chai": "^4.3.10",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "ethers": "^6.11.0",
    "hardhat": "^2.22.0",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-deploy": "^0.12.0",
    "hardhat-gas-reporter": "^1.0.10",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "prettier-plugin-solidity": "^1.3.1",
    "solhint": "^4.1.1",
    "solhint-plugin-prettier": "^0.1.0",
    "solidity-coverage": "^0.8.0",
    "ts-node": "^10.9.0",
    "typechain": "^8.3.0",
    "typescript": "^5.3.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.sol": [
      "solhint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
