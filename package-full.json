{
  "name": "private-traffic-aggregator",
  "version": "2.0.0",
  "description": "Private Traffic Data Aggregation using Zama FHE encryption with full development tooling",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:gas": "REPORT_GAS=true hardhat test",
    "deploy:local": "hardhat deploy --network localfhe",
    "deploy:sepolia": "hardhat deploy --network sepolia",
    "node": "hardhat node",
    "clean": "hardhat clean",
    "typechain": "hardhat typechain",
    "size": "hardhat size-contracts",
    "start": "npx http-server public -p 3000 -c-1 --cors",
    "build": "npm run compile && npm run typechain",
    "verify:sepolia": "hardhat verify --network sepolia"
  },
  "keywords": [
    "blockchain",
    "fhe",
    "privacy",
    "traffic",
    "zama",
    "hardhat",
    "ethereum"
  ],
  "author": "Private Traffic Analytics",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@fhevm/hardhat-plugin": "^0.2.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.0",
    "chai": "^4.3.10",
    "dotenv": "^16.3.1",
    "ethers": "^6.9.0",
    "hardhat": "^2.19.0",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-deploy": "^0.12.0",
    "hardhat-gas-reporter": "^1.0.9",
    "typechain": "^8.3.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@fhevm/solidity": "^0.5.0",
    "fhevmjs": "^0.5.0"
  }
}
